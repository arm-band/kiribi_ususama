<%- include('_functions_filename') -%>
<%- include('_eyecatch_header') -%>
<%- include('_date') -%>
<%
config = config;
commonVar = commonVar;
plugins = plugins;
DEV_MODE = DEV_MODE;
copyRightYear = funcCopyRightYear(config);
fileNameArray = funcFileNameProcedure(filename);
id = funcFileNameId(filename, fileNameArray);
relPath = funcFileNamePath(filename, fileNameArray, id);
if(id !== 'article') {
    attributes = null;
}
headerString = funcEyecatchHeader(config, parameters, id, attributes);
navbarHeight = commonVar['navbar-height'].replace(/\px/g, ''); //commonVarの中のナビゲーションバーの高さの値からpxを取り除き、数字だけにする(bodyタグのdata-offsetに使用)
flagFixedAnchor = 'false';
if(plugins.fixedanchor_scss) {
    flagFixedAnchor = 'true';
}
-%>