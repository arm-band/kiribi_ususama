<% for(let i of Object.keys(newsBlock)) {
    const datetime = funcDateTimeFormat(newsBlock[i].date, '', 'tz');
    const showDt = funcDateTimeFormat(newsBlock[i].date, '/', 'ymd');
    const articleId = funcArticleUrl('', newsBlock[i], true);
    const articleUrl = funcArticleUrl('', newsBlock[i]);
    let excerpt = '';
    if(newsBlock[i].excerpt !== null && newsBlock[i].excerpt !== undefined) {
        excerpt = newsBlock[i].excerpt;
    }
%>
                <div class="col-md-12 col-lg-6 col-xl-4 mb-4 u-news_card o-news_card">
                    <article class="news_article" id="<%= articleId %>">
                        <div class="card h-100">
<% if(newsBlock[i].thumbnail !== null && newsBlock[i].thumbnail !== undefined) { %>
                            <img class="card-img-top img-fluid" src="<%= relPath %><%- config.param.ogp.newsthumbnail %><%- newsBlock[i].thumbnail %>" alt="<%= newsBlock[i].title %>">
<% } %>
                            <div class="card-body p-3">
                                <h3 class="news_header"><a href="<%= relPath %><%= articleUrl %>"><%= newsBlock[i].title %></a></h3>
                                <div class="news_contents">
                                    <p><%- excerpt %></p>
                                </div>
                            </div>
                            <div class="card-footer p-3 text-right bg-white border-0">
                                <i class="fas fa-fw fa-history" aria-hidden="true"></i><time class="news_createtime" datetime="<%= datetime %>"><%= showDt %></time>
                            </div>
                        </div>
                    </article>
                </div>
<% } %>