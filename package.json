{
    "name": "kiribi_ususama",
    "version": "5.1.2-4.3.1",
    "description": "gulpを使用した小・中規模Webサイト制作用のボイラープレートです。",
    "main": "gulpfile.js",
    "scripts": {
        "om": "mkdirp dist bin/styleguide/dist src/contents src/scss/assets bin/config/subterranean",
        "krodhana": "node ./bin/saicho",
        "hum": "npm-run-all -s kongou eshaku riken",
        "jah": "node ./bin/sculpture",
        "svaha": "gulp",
        "kongou": "npm-run-all -p kashou zanmai gue shingon",
        "kashou": "cpx \"./node_modules/bootstrap-honoka/scss/*.scss\" \"./src/scss/assets/bootstrap\"",
        "zanmai": "cpx \"./node_modules/bootstrap-honoka/scss/honoka/_honoka.scss\" \"./src/scss/assets/bootstrap/honoka\"",
        "gue": "cpx \"./node_modules/bootstrap-honoka/scss/honoka/*.scss\" \"./src/scss/assets/bootstrap/honoka/honoka\"",
        "shingon": "cpx \"./node_modules/bootstrap/scss/**/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss\"",
        "katen": "node ./bin/daishi/dist/bin/www",
        "agni": "node ./bin/agni",
        "eshaku": "npm-run-all -p katen agni",
        "riken": "node ./bin/riken",
        "fujoketsu": "rimraf ./src/scss/assets/ ./src/js/concat/ ./src/scss/util/_var.scss ./src/img/lightbox/ ./dist/ ./bin/daishi/_htmlPager.html ./bin/styleguide/dist/ ./bin/config/subterranean/ ./package-lock.json",
        "lightbox:css": "mkdirp src/scss/assets/lightbox",
        "lightbox:img": "cpx \"./node_modules/lightbox2/dist/images/*\" \"./src/img/lightbox\"",
        "slick": "npm-run-all -p slick:*",
        "slick:gif": "cpx \"./node_modules/slick-carousel/slick/ajax-loader.gif\" \"./dist/css\"",
        "slick:fonts": "cpx \"./node_modules/slick-carousel/slick/fonts/*\" \"./dist/css/fonts\"",
        "gulp": "gulp",
        "opener": "opener",
        "start": "npm-run-all -s om krodhana hum jah svaha",
        "test": "npm-run-all -s plugin gulp",
        "deploy": "gulp ftp",
        "guide": "gulp styleguide",
        "setting": "npm run eshaku",
        "plugin": "npm run riken",
        "new": "node ./bin/sculpture",
        "sitemap": "gulp sitemap",
        "sitexml": "gulp sitemapxml",
        "delete": "npm run fujoketsu"
    },
    "author": "Arm Band",
    "license": "ISC",
    "browserslist": [
        "last 2 version",
        "iOS >= 10.0",
        "Android >= 5.0"
    ],
    "dependencies": {
        "brand-colors": "^2.0.1",
        "bootstrap": "^4.3.1",
        "bootstrap-honoka": "^4.3.1",
        "bowser": "^2.4.0",
        "jquery": "^3.4.1",
        "jquery.easing": "^1.4.1",
        "lightbox2": "^2.11.0",
        "slick-carousel": "^1.8.1"
    },
    "devDependencies": {
        "browser-sync": "^2.26.7",
        "cpx": "^1.5.0",
        "express": "^4.17.1",
        "front-matter": "^3.0.2",
        "ftp-deploy": "^2.3.3",
        "gulp": "^4.0.2",
        "gulp-concat": "^2.6.1",
        "gulp-connect-php": "^1.0.3",
        "gulp-data": "^1.3.1",
        "gulp-ejs": "^4.1.1",
        "gulp-frontnote": "^2.0.1",
        "gulp-imagemin": "^6.0.0",
        "gulp-notify": "^3.2.0",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "^1.4.0",
        "gulp-sass": "^4.0.2",
        "gulp-autoprefixer": "^6.1.0",
        "gulp-uglify-es": "^1.0.4",
        "gulp-watch": "^5.0.1",
        "marked": "^0.6.2",
        "mkdirp": "^0.5.1",
        "npm-run-all": "^4.1.5",
        "require-dir": "^1.2.0",
        "rimraf": "^2.6.3",
        "rss": "^1.2.2",
        "yaml": "^1.6.0",
        "cookie-parser": "^1.4.4",
        "debug": "^4.1.1",
        "ejs": "^2.6.2",
        "http-errors": "^1.7.2",
        "morgan": "^1.9.1",
        "opener": "^1.5.1"
    },
    "engines": {
        "node": ">= 9.0.0",
        "npm": ">= 4.0.0"
    }
}
