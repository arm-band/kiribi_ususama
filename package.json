{
    "name": "kiribi_ususama",
    "version": "8.3.2-4.4.0",
    "description": "gulpを使用した小・中規模Webサイト制作用のフレームワークです。",
    "main": "gulpfile.js",
    "scripts": {
        "om": "mkdirp dist dist/webfonts dist/assets dist/images bin/styleguide/dist src/contents bin/config/subterranean",
        "krodhana": "node ./bin/saicho",
        "hum": "run-s kakaishu eshaku riken gueshingon",
        "jah": "node ./bin/sculpture",
        "svaha": "gulp",
        "gueshingon": "cpx \"./node_modules/@fortawesome/fontawesome-free/webfonts/**/*\" \"./dist/webfonts/\"",
        "kakaishu": "node ./bin/kakaishu",
        "katen": "node ./bin/daishi/dist/bin/www",
        "agni": "node ./bin/agni",
        "eshaku": "run-p katen agni",
        "riken": "node ./bin/riken",
        "fujoketsu": "rimraf ./src/scss/assets/ ./src/js/concat/ ./src/scss/util/_var.scss ./src/img/lightbox/ ./dist/ ./bin/styleguide/dist/ ./bin/config/subterranean/",
        "lightbox:img": "cpx \"./node_modules/lightbox2/dist/images/*\" \"./dist/images\"",
        "slick:gif": "cpx \"./node_modules/slick-carousel/slick/ajax-loader.gif\" \"./dist/css\"",
        "slick:fonts": "cpx \"./node_modules/slick-carousel/slick/fonts/*\" \"./dist/css/fonts\"",
        "gulp": "gulp",
        "opener": "opener",
        "lint:html": "htmllint ./dist/**/*.html",
        "lint:js": "eslint ./src/js/**/*.js",
        "lint:scss": "stylelint ./src/scss/**/*.scss",
        "lint:http": "node ./bin/johari",
        "lint": "run-p lint:*",
        "fix:js": "eslint --fix ./src/js/**/*.js && prettier --config .prettierrc.json --write ./src/js/**/*.js",
        "fix:scss": "stylelint --fix ./src/scss/**/*.scss",
        "fix": "run-s fix:*",
        "start": "run-s om krodhana hum jah svaha",
        "restart": "run-s plugin gulp",
        "test": "npm run lint",
        "deploy": "gulp ftp",
        "guide": "gulp styleguide",
        "init": "run-s om kakaishu gueshingon",
        "setting": "npm run eshaku",
        "plugin": "npm run riken",
        "new": "node ./bin/sculpture",
        "sitemap": "gulp sitemap",
        "sitexml": "gulp sitemapxml",
        "search": "gulp sitesearch",
        "delete": "npm run fujoketsu",
        "om_": "rimraf ./dist/",
        "kurodano": "npm run om",
        "un": "npm run init",
        "jaku": "npm run riken",
        "sowaka": "gulp build",
        "tankai": "node ./bin/tankai",
        "build": "run-s om_ kurodano un jaku sowaka"
    },
    "author": "Arm Band",
    "license": "ISC",
    "dependencies": {
        "brand-colors": "^2.1.1",
        "bootstrap": "^4.6.0",
        "bootstrap-honoka": "^4.4.0",
        "bowser": "^2.11.0",
        "@fortawesome/fontawesome-free": "^5.15.3",
        "jquery": "^3.6.0",
        "jquery.easing": "^1.4.1",
        "lightbox2": "^2.11.3",
        "slick-carousel": "^1.8.1",
        "list.js": "^2.3.1"
    },
    "devDependencies": {
        "browser-sync": "^2.26.14",
        "cpx": "^1.5.0",
        "express": "^4.17.1",
        "front-matter": "^4.0.2",
        "ftp-deploy": "^2.4.1",
        "gulp": "^4.0.2",
        "gulp-connect-php": "^1.0.3",
        "gulp-data": "^1.3.1",
        "gulp-ejs": "^5.1.0",
        "gulp-frontnote": "^2.0.1",
        "gulp-htmlmin": "^5.0.1",
        "gulp-imagemin": "^7.1.0",
        "imagemin-mozjpeg": "^9.0.0",
        "imagemin-pngquant": "^9.0.2",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-svgo": "^9.0.0",
        "gulp-notify": "^4.0.0",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^4.1.0",
        "sass": "^1.32.12",
        "fibers": "^5.0.0",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-replace": "^1.1.2",
        "webpack": "^5.36.2",
        "webpack-stream": "^6.1.2",
        "terser-webpack-plugin": "^5.1.1",
        "clean-webpack-plugin": "^3.0.0",
        "marked": "^2.0.3",
        "mkdirp": "^1.0.4",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "rss": "^1.2.2",
        "yaml": "^1.10.2",
        "lodash.sortby": "^4.7.0",
        "dotenv": "^9.0.0",
        "cookie-parser": "^1.4.5",
        "debug": "^4.3.1",
        "ejs": "^3.1.6",
        "http-errors": "^1.8.0",
        "morgan": "^1.10.0",
        "opener": "^1.5.2",
        "htmllint-cli": "^0.0.7",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.3.0",
        "stylelint": "^13.13.1",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-scss": "^3.19.0",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-config-prettier": "^8.0.2",
        "prettier": "^2.2.1",
        "axios": "^0.21.1"
    },
    "resolutions": {
        "webpack": "^5.36.2"
    },
    "engines": {
        "node": ">=14.0.0",
        "npm": ">=6.14.4"
    },
    "engineStrict": true,
    "prettier": {}
}
