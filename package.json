{
    "name": "kiribi_ususama",
    "version": "6.11.0-4.3.1",
    "description": "gulpを使用した小・中規模Webサイト制作用のフレームワークです。",
    "main": "gulpfile.js",
    "scripts": {
        "om": "mkdirp dist dist/webfonts bin/styleguide/dist src/contents src/scss/assets bin/config/subterranean",
        "krodhana": "node ./bin/saicho",
        "hum": "npm-run-all -s kongou eshaku riken",
        "jah": "node ./bin/sculpture",
        "svaha": "gulp",
        "kongou": "npm-run-all -p kakaishu kashou zanmai gue shingon reiyoumon",
        "kashou": "cpx \"./node_modules/bootstrap-honoka/scss/*.scss\" \"./src/scss/assets/bootstrap\"",
        "zanmai": "cpx \"./node_modules/bootstrap-honoka/scss/honoka/_honoka.scss\" \"./src/scss/assets/bootstrap/honoka\"",
        "gue": "cpx \"./node_modules/bootstrap-honoka/scss/honoka/*.scss\" \"./src/scss/assets/bootstrap/honoka/honoka\"",
        "shingon": "cpx \"./node_modules/bootstrap/scss/**/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss\"",
        "reiyoumon": "cpx \"./node_modules/@fortawesome/fontawesome-free/webfonts/**/*\" \"./dist/webfonts/\"",
        "kakaishu": "node ./bin/kakaishu",
        "katen": "node ./bin/daishi/dist/bin/www",
        "agni": "node ./bin/agni",
        "eshaku": "npm-run-all -p katen agni",
        "riken": "node ./bin/riken",
        "fujoketsu": "rimraf ./src/scss/assets/ ./src/js/concat/ ./src/scss/util/_var.scss ./src/img/lightbox/ ./dist/ ./bin/styleguide/dist/ ./bin/config/subterranean/ ./package-lock.json",
        "lightbox:css": "mkdirp src/scss/assets/lightbox",
        "lightbox:img": "cpx \"./node_modules/lightbox2/dist/images/*\" \"./src/img/lightbox\"",
        "slick": "npm-run-all -p slick:*",
        "slick:gif": "cpx \"./node_modules/slick-carousel/slick/ajax-loader.gif\" \"./dist/css\"",
        "slick:fonts": "cpx \"./node_modules/slick-carousel/slick/fonts/*\" \"./dist/css/fonts\"",
        "gulp": "gulp",
        "opener": "opener",
        "lint:html": "htmllint ./dist/**/*.html",
        "lint:js": "eslint ./src/js/**/*.js",
        "lint:scss": "stylelint ./src/scss/**/*.scss",
        "lint:http": "node ./bin/johari",
        "lint": "npm-run-all -s lint:*",
        "prettier:js": "eslint --fix ./src/js/**/*.js",
        "prettier:scss": "stylelint --fix ./src/scss/**/*.scss",
        "prettier": "npm-run-all -s prettier:*",
        "start": "npm-run-all -s om krodhana hum jah svaha",
        "restart": "npm-run-all -s plugin gulp",
        "test": "npm run lint",
        "deploy": "gulp ftp",
        "guide": "gulp styleguide",
        "init": "npm-run-all -s om kongou",
        "setting": "npm run eshaku",
        "plugin": "npm run riken",
        "new": "node ./bin/sculpture",
        "sitemap": "gulp sitemap",
        "sitexml": "gulp sitemapxml",
        "search": "gulp sitesearch",
        "delete": "npm run fujoketsu",
        "kurodano": "npm run kongou",
        "unjaku": "npm run riken",
        "sowaka": "gulp build",
        "tankai": "node ./bin/tankai",
        "build": "npm-run-all -s om kurodano unjaku sowaka"
    },
    "author": "Arm Band",
    "license": "ISC",
    "dependencies": {
        "brand-colors": "^2.1.1",
        "bootstrap": "^4.4.1",
        "bootstrap-honoka": "^4.3.1",
        "@fortawesome/fontawesome-free": "^5.13.0",
        "jquery": "^3.5.0",
        "jquery.easing": "^1.4.1",
        "lightbox2": "^2.11.1",
        "slick-carousel": "^1.8.1",
        "list.js": "^1.5.0"
    },
    "devDependencies": {
        "browser-sync": "^2.26.7",
        "cpx": "^1.5.0",
        "express": "^4.17.1",
        "front-matter": "^3.2.1",
        "ftp-deploy": "^2.3.7",
        "gulp": "^4.0.2",
        "gulp-concat": "^2.6.1",
        "gulp-connect-php": "^1.0.3",
        "gulp-data": "^1.3.1",
        "gulp-ejs": "^5.1.0",
        "gulp-frontnote": "^2.0.1",
        "gulp-htmlmin": "^5.0.1",
        "gulp-imagemin": "^7.1.0",
        "imagemin-mozjpeg": "^8.0.0",
        "imagemin-pngquant": "^8.0.0",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-svgo": "^7.1.0",
        "gulp-notify": "^3.2.0",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^4.1.0",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-replace": "^1.0.0",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-uglify-es": "^2.0.0",
        "gulp-watch": "^5.0.1",
        "marked": "^1.1.0",
        "mkdirp": "^1.0.4",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "rss": "^1.2.2",
        "yaml": "^1.10.0",
        "lodash.sortby": "^4.7.0",
        "dotenv": "^8.2.0",
        "cookie-parser": "^1.4.5",
        "debug": "^4.1.1",
        "ejs": "^3.1.3",
        "http-errors": "^1.7.3",
        "morgan": "^1.10.0",
        "opener": "^1.5.1",
        "htmllint-cli": "^0.0.7",
        "eslint": "^7.0.0",
        "stylelint": "^13.4.1",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-scss": "^3.17.2",
        "prettier": "^2.0.5",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-prettier": "^3.1.3",
        "stylelint-prettier": "^1.1.2",
        "stylelint-config-prettier": "^8.0.1",
        "axios": "^0.19.2"
    },
    "engines": {
        "node": ">=12.0.0",
        "npm": ">=6.0.0"
    },
    "engineStrict": true,
    "prettier": {}
}
