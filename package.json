{
    "name": "kiribi_ususama",
    "version": "4.6.0-4.3.1",
    "description": "gulpを使用した小・中規模Webサイト制作用のボイラープレートです。",
    "main": "gulpfile.js",
    "scripts": {
        "om": "mkdirp sg/dist src/scss/assets src/scss/assets/bootstrap src/scss/assets/bootstrap/honoka src/scss/assets/bootstrap/honoka/bootstrap src/scss/assets/bootstrap/honoka/honoka src/scss/assets/bootstrap/honoka/bootstrap/scss src/scss/assets/bootstrap/honoka/bootstrap/scss/mixins src/scss/assets/bootstrap/honoka/bootstrap/scss/utilities src/scss/assets/bootstrap/honoka/bootstrap/scss/vendor bin/subterranean",
        "krodhana": "node ./bin/saicho",
        "hum": "npm-run-all -s kongou eshaku",
        "jah": "node ./bin/katou",
        "svaha": "gulp",
        "kongou": "npm-run-all -p vidya raja myouou kashou zanmai gue shingon",
        "vidya": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/*.scss\" \"./src/scss/assets/bootstrap\"",
        "raja": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/honoka/_honoka.scss\" \"./src/scss/assets/bootstrap/honoka\"",
        "myouou": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/honoka/*.scss\" \"./src/scss/assets/bootstrap/honoka/honoka\"",
        "kashou": "copyfiles -f \"./node_modules/bootstrap/scss/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss\"",
        "zanmai": "copyfiles -f \"./node_modules/bootstrap/scss/mixins/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss/mixins\"",
        "gue": "copyfiles -f \"./node_modules/bootstrap/scss/utilities/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss/utilities\"",
        "shingon": "copyfiles -f \"./node_modules/bootstrap/scss/vendor/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss/vendor\"",
        "katen": "node ./dist/daishi/bin/www",
        "agni": "node ./bin/agni",
        "eshaku": "npm-run-all -p katen agni",
        "fujoketsu": "rimraf ./src/scss/assets/ ./src/js/concat/ ./src/scss/util/_var.scss ./dist/*.html ./dist/news/ ./dist/css/ ./dist/js/ ./dist/img/ ./dist/*.ico ./dist/*.png ./dist/*.xml ./dist/daishi/_htmlPager.html ./sg/dist/ ./bin/subterranean/ ./package-lock.json",
        "gulp": "gulp",
        "opener": "opener",
        "start": "npm-run-all -s om krodhana hum jah svaha",
        "test": "gulp",
        "deploy": "gulp ftp",
        "guide": "gulp styleguide",
        "setting": "npm run eshaku",
        "new": "node ./bin/sculpture"
    },
    "author": "Arm Band",
    "license": "ISC",
    "devDependencies": {
        "brand-colors": "^2.0.1",
        "bootstrap": "^4.3.1",
        "bootstrap-honoka": "^4.3.1",
        "bowser": "^2.1.0",
        "browser-sync": "^2.26.3",
        "copyfiles": "^2.1.0",
        "express": "^4.16.4",
        "front-matter": "^3.0.1",
        "ftp-deploy": "^2.3.1",
        "gulp": "^4.0.0",
        "gulp-concat": "^2.6.1",
        "gulp-connect-php": "^1.0.3",
        "gulp-data": "^1.3.1",
        "gulp-ejs": "^3.3.0",
        "gulp-frontnote": "^2.0.1",
        "gulp-imagemin": "^5.0.3",
        "gulp-notify": "^3.2.0",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "^1.4.0",
        "gulp-sass": "^4.0.2",
        "gulp-autoprefixer": "^6.0.0",
        "gulp-uglify-es": "^1.0.4",
        "gulp-watch": "^5.0.1",
        "jquery": "^3.3.1",
        "jquery.easing": "^1.4.1",
        "marked": "^0.6.1",
        "mkdirp": "^0.5.1",
        "npm-run-all": "^4.1.5",
        "require-dir": "^1.2.0",
        "rimraf": "^2.6.3",
        "rss": "^1.2.2",
        "yaml": "^1.3.2",
        "cookie-parser": "^1.4.4",
        "debug": "^4.1.1",
        "ejs": "^2.6.1",
        "http-errors": "^1.7.2",
        "morgan": "^1.9.1",
        "opener": "^1.5.1"
    },
    "engines": {
        "node": ">= 8.5.0",
        "npm": ">= 4.0.0"
    }
}
