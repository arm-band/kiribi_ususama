{
    "name": "kiribi_ususama",
    "version": "4.0.2-4.1.3",
    "description": "gulpを使用した小・中規模Webサイト制作用のボイラープレートです。",
    "main": "gulpfile.js",
    "scripts": {
        "om": "mkdirp sg/dist src/scss/assets src/scss/assets/bootstrap src/scss/assets/bootstrap/honoka src/scss/assets/bootstrap/honoka/bootstrap src/scss/assets/bootstrap/honoka/honoka src/scss/assets/bootstrap/honoka/bootstrap/scss src/scss/assets/bootstrap/honoka/bootstrap/scss/mixins src/scss/assets/bootstrap/honoka/bootstrap/scss/utilities bin/subterranean",
        "krodhana": "node ./bin/saicho",
        "hum": "npm-run-all -s vidya raja kashou zanmai gue shingon eshaku",
        "jah": "node ./bin/katou",
        "svaha": "gulp init",
        "vidya": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/*.scss\" \"./src/scss/assets/bootstrap\"",
        "raja": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/honoka/_honoka.scss\" \"./src/scss/assets/bootstrap/honoka\"",
        "kashou": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/honoka/*.scss\" \"./src/scss/assets/bootstrap/honoka/honoka\"",
        "zanmai": "copyfiles -f \"./node_modules/bootstrap/scss/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss\"",
        "gue": "copyfiles -f \"./node_modules/bootstrap/scss/mixins/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss/mixins\"",
        "shingon": "copyfiles -f \"./node_modules/bootstrap/scss/utilities/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss/utilities\"",
        "katen": "node ./dist/daishi/bin/www",
        "agni": "node ./bin/agni",
        "eshaku": "npm-run-all -p katen agni",
        "fujoketsu": "rimraf ./src/scss/assets/ ./src/js/concat/ ./src/scss/util/_var.scss ./dist/*.html ./dist/news/ ./dist/css/ ./dist/js/ ./dist/img/ ./dist/*.ico ./dist/*.png ./dist/*.xml ./sg/dist/ ./bin/subterranean/ ./package-lock.json",
        "gulp": "gulp",
        "opener": "opener",
        "start": "npm-run-all -s om krodhana hum jah svaha",
        "test": "gulp server"
    },
    "author": "Arm Band",
    "license": "ISC",
    "devDependencies": {
        "brand-colors": "^2.0.1",
        "bootstrap": "^4.1.3",
        "bootstrap-honoka": "^4.1.3",
        "bowser": "^1.9.4",
        "browser-sync": "^2.24.4",
        "copyfiles": "^2.0.0",
        "express": "^4.16.4",
        "front-matter": "^3.0.0",
        "ftp-deploy": "^2.1.3",
        "gulp": "next",
        "gulp-concat": "^2.6.1",
        "gulp-connect-php": "^1.0.3",
        "gulp-data": "^1.3.1",
        "gulp-ejs": "^3.1.3",
        "gulp-frontnote": "^2.0.1",
        "gulp-imagemin": "^4.1.0",
        "gulp-notify": "^3.2.0",
        "gulp-plumber": "^1.2.0",
        "gulp-rename": "^1.2.2",
        "gulp-sass": "^4.0.1",
        "gulp-autoprefixer": "^5.0.0",
        "gulp-uglify": "^3.0.0",
        "gulp-watch": "^5.0.1",
        "jquery": "^3.3.1",
        "jquery.easing": "^1.4.1",
        "marked": "^0.5.1",
        "mkdirp": "^0.5.1",
        "npm-run-all": "^4.1.3",
        "require-dir": "^1.1.0",
        "rimraf": "^2.6.2",
        "rss": "^1.2.2",
        "yaml": "^1.0.0-rc.8",
        "cookie-parser": "^1.4.3",
        "debug": "^2.6.9",
        "ejs": "^2.5.7",
        "http-errors": "^1.6.2",
        "morgan": "^1.9.0",
        "opener": "^1.5.1"
    }
}
