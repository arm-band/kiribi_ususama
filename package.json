{
    "name": "kiribi_ususama",
    "version": "3.12.1-4.1.3",
    "description": "gulpを使用した小・中規模Webサイト制作用のボイラープレートです。",
    "main": "gulpfile.js",
    "scripts": {
        "kashouzanmai": "mkdirp dist sg/dist admin admin/bin admin/css admin/js src/scss/assets src/scss/assets/bootstrap src/scss/assets/bootstrap/honoka src/scss/assets/bootstrap/honoka/bootstrap src/scss/assets/bootstrap/honoka/honoka src/scss/assets/bootstrap/honoka/bootstrap/scss src/scss/assets/bootstrap/honoka/bootstrap/scss/mixins src/scss/assets/bootstrap/honoka/bootstrap/scss/utilities gulp/subterranean",
        "gueshingon": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/*.scss\" \"./src/scss/assets/bootstrap\"",
        "om": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/honoka/_honoka.scss\" \"./src/scss/assets/bootstrap/honoka\"",
        "krodhana": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/honoka/*.scss\" \"./src/scss/assets/bootstrap/honoka/honoka\"",
        "hum": "copyfiles -f \"./node_modules/bootstrap/scss/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss\"",
        "jah": "copyfiles -f \"./node_modules/bootstrap/scss/mixins/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss/mixins\"",
        "svaha": "copyfiles -f \"./node_modules/bootstrap/scss/utilities/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss/utilities\"",
        "ususama": "npm-run-all -s kashouzanmai gueshingon om krodhana hum jah svaha",
        "gulp": "gulp",
        "fujoketsu": "rimraf ./src/scss/assets/ ./src/js/concat/ ./src/scss/util/_var.scss ./dist/ ./sg/dist/ ./package-lock.json"
    },
    "author": "Arm Band",
    "license": "ISC",
    "devDependencies": {
        "brand-colors": "^2.0.1",
        "bootstrap": "^4.1.3",
        "bootstrap-honoka": "^4.1.3",
        "bowser": "^1.9.4",
        "browser-sync": "^2.24.4",
        "copyfiles": "^2.0.0",
        "front-matter": "^3.0.0",
        "ftp-deploy": "^2.1.3",
        "gulp": "next",
        "gulp-concat": "^2.6.1",
        "gulp-connect-php": "^1.0.3",
        "gulp-data": "^1.3.1",
        "gulp-ejs": "^3.1.3",
        "gulp-frontnote": "^2.0.1",
        "gulp-imagemin": "^4.1.0",
        "gulp-notify": "^3.2.0",
        "gulp-plumber": "^1.2.0",
        "gulp-rename": "^1.2.2",
        "gulp-sass": "^4.0.1",
        "gulp-autoprefixer": "^5.0.0",
        "gulp-uglify": "^3.0.0",
        "gulp-watch": "^5.0.1",
        "jquery": "^3.3.1",
        "jquery.easing": "^1.4.1",
        "marked": "^0.5.1",
        "mkdirp": "^0.5.1",
        "npm-run-all": "^4.1.3",
        "readline-sync": "^1.4.9",
        "require-dir": "^1.1.0",
        "rimraf": "^2.6.2",
        "rss": "^1.2.2",
        "yaml": "1.0.0-rc.8"
    }
}