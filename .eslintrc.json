{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "$": false,
        "mobileSafariRequiem": false,
        "pageTop": false,
        "slickCarousel": false,
        "fixedanchor_js": false,
        "List": false
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "off"
    }
}